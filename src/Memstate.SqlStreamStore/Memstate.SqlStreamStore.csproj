<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackOnBuild>true</PackOnBuild>
        <PackageVersion>0.2</PackageVersion>
        <Authors>Devrex Labs</Authors>
        <Description>Storage provider based on SqlStreamStore allowing the use of any sql backend supported by SqlStreamStore. MsSql, PostgreSQL</Description>
        <Copyright>Devrex Labs</Copyright>
        <Owners>
Devrex Labs</Owners>
        <Summary>Storage provider for Memstate based on SqlStreamStore targeting any sql database supported by SqlStreamStore</Summary>
        <Title>Memstate.SqlStreamStore</Title>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Configurations>Debug;Release;Test</Configurations>
        <Platforms>AnyCPU</Platforms>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\Memstate.Core\Memstate.Core.csproj" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="SqlStreamStore" Version="1.2.0" />
      <PackageReference Include="SqlStreamStore.Postgres" Version="1.2.0-beta.8" />
    </ItemGroup>
    
</Project>
